	<script>
		// grabs json data from url, thanks https://stackoverflow.com/a/35970894
		var getJSON = function (url, callback) {
			var xhr = new XMLHttpRequest();
			xhr.open("GET", url, true);
			xhr.responseType = "json";
			xhr.onload = function () {
				var status = xhr.status;
				if (status === 200) {
					callback(null, xhr.response);
				} else {
					callback(status, xhr.response);
				}
			};
			xhr.send();
		};

		getJSON(
			"https://corsproxy.io/?https://scoresaber.com/api/player/76561198870563581/basic",
			function (err, data) {
				if (err !== null) {
					document.getElementById("scoresaberRank").innerText = "RANK UNAVAILABLE";
				} else {
					document.getElementById("scoresaberRank").innerText = "RANK #" + data.rank;
				}
